{
    "program": "binary_adder.p4",
    "pipeline": {
      "v1model": {
        "my_switch": {
          "ingress": "MyIngress",
          "egress": "MyEgress"
        }
      }
    },
    "topology": {
      "links": [
        ["h1", "s1"],
        ["h2", "s1"]
      ]
    },
    "hosts": {
      "h1": {
        "cmd": "ifconfig h1-eth0 10.0.0.1/24"
      },
      "h2": {
        "cmd": "ifconfig h2-eth0 10.0.0.2/24"
      }
    }
  }
  